                        <PickerRow color="#0099ff" title="13 divers" />
                        <PickerRow color="#0099ff" title="14 divers" />
                        <PickerRow color="#0099ff" title="15 divers" />
                        <PickerRow color="#0099ff" title="16 divers" />
                        <PickerRow color="#0099ff" title="17 divers" />
                        <PickerRow color="#0099ff" title="18 divers" />
                        <PickerRow color="#0099ff" title="19 divers" />
                        <PickerRow color="#0099ff" title="20 divers" />
                    </PickerColumn>
                </Picker>
                <Button id="speciesBtn" onClick="openSpecies"></Button>
                <Button id="sign_out" onClick="sign_out"></Button>
            </View>
        </View>
    </Window>
</Alloy>