<Alloy>
    <Window id="surveyWin" title="Survey">
        <!--Label id="surveyTitle">Survey</Label-->
        <View id="header">
            <Button id="logo"></Button>
            <Label id="create"></Label>
            <!--Label id="reg_btn"></Label-->
        </View>
        <View id="body">
            <View id="up">
                <Label id="label1" />
                <Label id="label2" />
                <Picker class="picker" id="picker" selectionIndicator="true" useSpinner="false">
                    <PickerColumn id="column1" width="30%"></PickerColumn>
                </Picker>

                <Label id="label3" />
                <Picker class="picker" id="picker2" selectionIndicator="true" useSpinner="false">
                    <PickerColumn id="column2" width="30%">
                        <PickerRow color="#0099ff" title="10 minutes" />
                        <PickerRow color="#0099ff" title="20 minutes" />
                        <PickerRow color="#0099ff" title="30 minutes" />
                        <PickerRow color="#0099ff" title="40 minutes" />
                        <PickerRow color="#0099ff" title="50 minutes" />
                        <PickerRow color="#0099ff" title="60 minutes" />
                        <PickerRow color="#0099ff" title="70 minutes" />
                        <PickerRow color="#0099ff" title="80 minutes" />
                        <PickerRow color="#0099ff" title="90 minutes" />
                        <PickerRow color="#0099ff" title="100 minutes" />
                        <PickerRow color="#0099ff" title="110 minutes" />
                        <PickerRow color="#0099ff" title="120 minutes" />
                    </PickerColumn>
                </Picker>
                <Label id="label4" />

                <Picker class="picker" id="picker3" selectionIndicator="true" useSpinner="false">
                    <PickerColumn id="column3" width="30%">
                        <PickerRow color="#0099ff" title="2 divers" />
